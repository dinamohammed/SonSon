<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <record id="paperformat_check_qnb" model="report.paperformat">
            <field name="name">QNB Check Letter</field>
            <field name="default" eval="False" />
            <field name="format">custom</field>
            <field name="page_height">83</field>
            <field name="page_width">167</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>
        
        <report id="action_print_qnb_check_top"
            string="Print QNB Check (Top)"
            model="account.payment"
            name="qnb_check_print.print_check_top_new"
            file="qnb_check_print.print_check_top_new"
            report_type="qweb-pdf"
            attachment_use="True"
            attachment="'qnb_'+(object.partner_id and object.partner_id.name or 'Internal')+'_'+(object.name)+'_top.pdf'"
            paperformat="qnb_check_print.paperformat_check_qnb"
            print_report_name="'QNB - %s' % (object.partner_id.name or '',)"
            menu="False"
        />
        
    </data>
</odoo>